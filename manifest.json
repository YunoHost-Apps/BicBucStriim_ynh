{
    "name": "BicBucStriim",
    "id": "bicbucstriim",
    "packaging_format": 1,
    "description": {
        "en": "Digital books streamer, providing web-based access to your e-book collection",
        "fr": "Interface web pour accéder à votre collection d'e-book"
    },
    "version": "1.5.3~ynh3",
    "url": "http://projekte.textmulch.de/bicbucstriim/",
    "upstream": {
        "license": "MIT",
        "website": "http://projekte.textmulch.de/bicbucstriim/",
        "code": "https://github.com/rvolz/BicBucStriim/"
    },
    "license": "MIT",
    "maintainer": {
        "name": "Alex",
        "email": "apulido@free.fr"
    },
    "requirements": {
        "yunohost": ">> 11.2"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php7.3-fpm"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/bbs",
                "default": "/bbs"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
